        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var user_id = current.sys_id;
		
	var course = new GlideRecord('x_1805432_mock_ski_course');
	course.query();

	var course_team = new GlideRecord('x_1805432_mock_ski_m2m_courses_team');

	while(course.next()){
		course_team.initialize();
		course_team.course = course.sys_id;
		course_team.intern = user_id;
		course_team.status = 'opened';

		course_team.insert();
	}
})(current, previous);]]></script>
